{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/coin-gecko/(.*)": "<rootDir>/../libs/coin-gecko/src/$1",
    "@app/coin-gecko": "<rootDir>/../libs/coin-gecko/src",
    "@app/prisma/(.*)": "<rootDir>/../libs/prisma/src/$1",
    "@app/prisma": "<rootDir>/../libs/prisma/src"
  }
}